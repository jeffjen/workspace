# include github git-completion
if [ -f ~/.workspaceenv/git-completion.bash ]; then
    . ~/.workspaceenv/git-completion.bash
fi

# include github git-prompt
if [ -f ~/.workspaceenv/git-prompt.sh ]; then
    . ~/.workspaceenv/git-prompt.sh
fi

# include machine completion
if [ -f ~/.workspaceenv/machine-completion.bash ]; then
    source ~/.workspaceenv/machine-completion.bash
fi

# include machine wrapper
if [ -f ~/.workspaceenv/machine-wrapper.bash ]; then
    source ~/.workspaceenv/machine-wrapper.bash
fi

# include machine prompt
if [ -f ~/.workspaceenv/machine-prompt.bash ]; then
    source ~/.workspaceenv/machine-prompt.bash
fi

# include docker-machine completion
if [ -f ~/.workspaceenv/machine-completion.bash ]; then
    source ~/.workspaceenv/docker-machine.bash
fi

# include docker-machine wrapper
if [ -f ~/.workspaceenv/machine-wrapper.bash ]; then
    source ~/.workspaceenv/docker-machine-wrapper.bash
fi

# include docker-machine prompt
if [ -f ~/.workspaceenv/machine-prompt.bash ]; then
    source ~/.workspaceenv/docker-machine-prompt.bash
fi

# include docker completion
if [ -f ~/.workspaceenv/docker.bash ]; then
    source ~/.workspaceenv/docker.bash
fi
