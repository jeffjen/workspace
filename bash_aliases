# This is the alias file for bash shell startup

alias cls='clear'
alias ll='ls -l'
alias la='ls -A'
alias lla='ls -lA'
alias cd.='cd ../'
alias cd..='cd ../../'
alias cd...='cd ../../../'
alias cd....='cd ../../../../'
alias cd.....='cd ../../../../../'

alias egrep='egrep --color'

alias maserati='ssh -Y $USER@maserati'
alias vbox='ssh -Y $USER@jeffjen-VirtualBox.local'

alias cp='cp -v'
